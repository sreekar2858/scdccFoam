FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    arch        "LSB;label=32;scalar=64";
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];


internalField   uniform 101325;

boundaryField
{
    Top
    {
        type            totalPressure;
        rho             rho;
        psi             none;
        gamma           1;
        p0              uniform 101325;
        value           uniform 101325;
    }
    Bottom
    {
        type            fixedFluxPressure;
        gradient        nonuniform 0;
        value           nonuniform 0;
    }
    htopWall
    {
        type            fixedFluxPressure;
        gradient        nonuniform 0;
        value           nonuniform 0;
    }
    condWall
    {
        type            fixedFluxPressure;
        gradient        nonuniform 0;
        value           nonuniform 0;
    }
    convWall
    {
        type            fixedFluxPressure;
        gradient        nonuniform 0;
        value           nonuniform 0;
    }
    LSymm
    {
        type            symmetryPlane;
    }
    frontAndBack
    {
        type            empty;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary2to0
    {
        type            processor;
        value           uniform 101325;
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform 101325;
    }
}


// ************************************************************************* //
